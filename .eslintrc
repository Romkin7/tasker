{
    "root": true,
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error"
    },
    "ignorePatterns": ["node_modules"],
    "parserOptions": {
        "ecmaVersion": 2019,
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },

    "env": {
        "es6": true
    }
}
